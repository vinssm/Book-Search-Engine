{
  "name": "googlebooks-app",
  "version": "1.0.0",
  "description": "- MERN Challenge: Book Search Engine.",
  "main": "server.js",
  "engines": {
    "node": "14.15.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinssm/Book-Search-Engine.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "apollo-server-express": "^3.6.6",
    "bcrypt": "^5.0.1",
    "engines": "^0.4.0",
    "express": "^4.17.3",
    "graphql": "^15.5.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mangodb": "^1.0.0",
    "mongoose": "^5.13.14",
    "nodemon": "^2.0.15",
    "react-router-dom": "^5.3.0"
  },
  "bugs": {
    "url": "https://github.com/vinssm/Book-Search-Engine/issues"
  },
  "homepage": "https://github.com/vinssm/Book-Search-Engine#readme"
}
